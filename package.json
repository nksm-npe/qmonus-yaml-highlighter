{
  "name": "qmonus-yaml",
  "displayName": "Qmonus YAML Highlighter",
  "description": "Syntax highlighting for YAML files with embedded Python code blocks in Qmonus format",
  "version": "0.0.1",
  "publisher": "qmonus",
  "engines": {
    "vscode": "^1.60.0"
  },
  "main": "./src/extension.js",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "yaml",
    "python",
    "qmonus",
    "syntax",
    "highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/yaml-embedded-python-highlighter"
  },
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "contributes": {
    "grammars": [{
      "scopeName": "source.qmonus-yaml",
      "path": "./syntaxes/qmonus-yaml.tmLanguage.json",
      "injectTo": ["source.yaml"]
    }]
  }
}
